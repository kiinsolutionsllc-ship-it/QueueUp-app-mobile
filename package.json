{
  "name": "queueup-mobile",
  "version": "5.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "node --max-old-space-size=8192 node_modules/.bin/expo start --clear",
    "start:memory": "node start-dev.js",
    "start:dev": "expo start --dev-client",
    "start:prod": "expo start --no-dev --minify",
    "start:fast": "expo start --no-dev --minify --tunnel",
    "android": "expo run:android",
    "android:release": "expo run:android --variant release",
    "android:fast": "expo run:android --variant debug --no-build-cache",
    "web": "expo start --web",
    "web:build": "expo export --platform web",
    "web:build:prod": "NODE_ENV=production expo export --platform web --minify",
    "firebase:build": "NODE_ENV=production expo export --platform web --output-dir dist",
    "firebase:deploy": "npm run firebase:build && firebase deploy",
    "firebase:serve": "npm run firebase:build && firebase serve",
    "build:dev": "npm run optimize && eas build --profile development",
    "build:preview": "npm run optimize && eas build --profile preview",
    "build:staging": "npm run optimize && eas build --profile staging",
    "build:prod": "npm run optimize && eas build --profile production",
    "build:android": "npm run optimize && eas build --platform android --profile production",
    "build:fast": "npm run clean:cache && npm run build:prod",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "prebuild:optimized": "npm run optimize && expo prebuild --clean",
    "doctor": "expo-doctor",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix --cache",
    "analyze": "npx react-native-bundle-visualizer",
    "analyze:size": "npm run build:prod && npx react-native-bundle-visualizer",
    "optimize:full": "npm run clean && npm run type-check",
    "clean": "expo r -c && rm -rf node_modules && npm install",
    "clean:cache": "expo r -c && npx react-native start --reset-cache",
    "clean:all": "rm -rf node_modules .expo android/app/build android/build ios/build .metro-cache && npm install",
    "submit:android": "eas submit --platform android",
    "validate": "npm run lint:check --max-warnings=1000",
    "validate:strict": "npm run type-check && npm run lint:check",
    "ios": "expo run:ios",
    "ios:release": "expo run:ios --configuration Release",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "build:ts": "tsc",
    "dev:ts": "tsc --watch",
    "perf:start": "npm run start:prod",
    "perf:analyze": "npm run analyze:size",
    "deploy:staging": "npm run build:staging && npm run submit:android",
    "deploy:prod": "npm run build:prod && npm run submit:android"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-google-signin/google-signin": "^10.1.2",
    "@react-native-voice/voice": "^3.2.4",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "@reduxjs/toolkit": "^2.9.0",
    "@shopify/react-native-skia": "2.2.12",
    "@stripe/stripe-react-native": "0.50.3",
    "@supabase/supabase-js": "^2.57.4",
    "@tanstack/react-query": "^5.87.4",
    "babel-preset-expo": "~54.0.0",
    "crypto-js": "^4.2.0",
    "expo": "~54.0.11",
    "expo-audio": "~1.0.0",
    "expo-auth-session": "~7.0.8",
    "expo-av": "~16.0.7",
    "expo-blur": "^15.0.7",
    "expo-build-properties": "~1.0.9",
    "expo-camera": "~17.0.8",
    "expo-crypto": "~15.0.7",
    "expo-device": "~8.0.9",
    "expo-document-picker": "~14.0.7",
    "expo-file-system": "~19.0.13",
    "expo-font": "~14.0.8",
    "expo-haptics": "^15.0.7",
    "expo-image-manipulator": "^14.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "^15.0.7",
    "expo-location": "~19.0.7",
    "expo-notifications": "~0.32.12",
    "expo-secure-store": "~15.0.7",
    "expo-sharing": "^14.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "metro": "^0.83.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-calendars": "^1.1302.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-credit-card-input": "^1.0.0",
    "react-native-device-info": "^10.11.0",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-gifted-chat": "^2.4.0",
    "react-native-google-places-autocomplete": "^2.5.7",
    "react-native-image-viewing": "^0.2.2",
    "react-native-keychain": "^8.1.3",
    "react-native-maps": "1.20.1",
    "react-native-modal": "^13.0.1",
    "react-native-paper": "^5.10.3",
    "react-native-paper-dates": "^0.10.0",
    "react-native-permissions": "^3.8.4",
    "react-native-ratings": "^8.1.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-super-grid": "^4.6.1",
    "react-native-svg": "^15.12.1",
    "react-native-tts": "^4.1.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "^0.5.1",
    "react-native-worklets-core": "^1.6.2",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "victory-native": "^41.20.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@types/react": "~19.1.10",
    "@types/react-native-vector-icons": "^6.4.18",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-plugin-import": "^1.13.8",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-imports": "^2.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "firebase-tools": "^13.0.0",
    "react-native-bundle-visualizer": "^2.0.0",
    "react-refresh": "^0.14.0",
    "typescript": "^5.9.2"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-tts",
          "react-native-vector-icons"
        ]
      }
    }
  },
  "overrides": {
    "react-native-safe-area-context": "5.6.1"
  },
  "private": true
}
