{
  "cli": {
    "version": ">= 5.9.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "NODE_ENV": "development",
        "EXPO_PUBLIC_APP_ENV": "development"
      },
      "android": {
        "gradleCommand": ":app:assembleDebug",
        "buildType": "apk"
      },
      "cache": {
        "disabled": false
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "NODE_ENV": "production",
        "EXPO_PUBLIC_APP_ENV": "preview"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "cache": {
        "disabled": false
      }
    },
    "staging": {
      "distribution": "internal",
      "env": {
        "NODE_ENV": "production",
        "EXPO_PUBLIC_APP_ENV": "staging"
      },
      "android": {
        "buildType": "aab",
        "gradleCommand": ":app:bundleRelease"
      },
      "cache": {
        "disabled": false
      }
    },
    "production": {
      "env": {
        "NODE_ENV": "production",
        "EXPO_PUBLIC_APP_ENV": "production"
      },
      "android": {
        "buildType": "aab",
        "gradleCommand": ":app:bundleRelease"
      },
      "cache": {
        "disabled": false
      }
    },
    "fast": {
      "extends": "production",
      "env": {
        "NODE_ENV": "production",
        "EXPO_PUBLIC_APP_ENV": "production",
        "EXPO_FAST_BUILD": "true"
      },
      "cache": {
        "disabled": false
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "production"
      }
    },
    "staging": {
      "android": {
        "track": "internal"
      }
    }
  }
}
